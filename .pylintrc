[MESSAGES CONTROL]

disable=locally-disabled,fixme,too-few-public-methods,too-many-public-methods,invalid-name,global-statement,too-many-ancestors,missing-docstring,no-self-use,redefined-outer-name,no-member,unused-argument

[FORMAT]

max-line-length=160

ignore-long-lines=^.*((https?:)|(pragma:)|(TODO:)).*$

[REPORTS]

reports=no

msg-template={msg_id} ({symbol}):{line:3d},{column}: {msg}
